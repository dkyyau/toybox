<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-8">

      <h1>Edit your request</h1>

      <%= simple_form_for @offer do |f| %>
      <%= f.input :start_date, as: :date, html5: true %>
      <%= f.input :end_date, as: :date, html5: true %>
      <%= f.button :submit, class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
